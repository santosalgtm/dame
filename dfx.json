      "candid": "src/dame_backend/dame_backend.did",
      "gzip": true,
      "post_install": ".kybra/dame_backend/post_install.sh",
      "tech_stack": {
        "cdk": {
          "kybra": {}
        },
        "language": {
          "python": {}
        }
      },
      "type": "custom",
      "wasm": ".kybra/dame_backend/dame_backend.wasm"
    },
    "dame_frontend": {
      "dependencies": [
        "dame_backend"
      ],
      "source": [
        "src/dame_frontend/dist"
